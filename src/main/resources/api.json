{
  "base-url": "https://purrbot.site/api",
  "endpoints": [
    {
      "name": "quote",
      "description": "Returns a image that looks like a message from a discord-user",
      "path": "/quote",
      "parameters": [
        "avatar",
        "color",
        "format",
        "name",
        "text",
        "time"
      ]
    },
    {
      "name": "status",
      "description": "Returns a user-avatar with a status-icon.",
      "path": "/status",
      "parameters": [
        "avatar",
        "status"
      ]
    },
    {
      "name": "welcome",
      "description": "Returns awelcome image with the users avatar, name and guild-count.",
      "path": "/welcome",
      "parameters": [
        "avatar",
        "color",
        "image",
        "name",
        "size"
      ]
    }
  ],
  "links": [
